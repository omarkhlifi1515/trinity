# Copy this file to `.env` and fill in real secrets. Keep `.env` out of version control.
SUPABASE_URL=https://your-supabase-project.supabase.co
SUPABASE_KEY=your-supabase-service-role-or-anon-key
OPENAI_API_KEY=sk-REPLACE_WITH_YOUR_KEY

# Optional: set the OpenAI model to use (defaults to gpt-3.5-turbo)
OPENAI_MODEL=gpt-3.5-turbo

# Supabase schema/table defaults (optional)
CHAT_LOGS_TABLE=chat_logs
PROFILES_TABLE=profiles