# AgentTrinity Environment Configuration
# Copy to .env for local development

# PostgreSQL database connection (required)
DATABASE_URL=postgresql://user:password@host:5432/trinity

# Agent server API key for authentication (required)
AGENT_API_KEY=your-secret-api-key-here

# OpenAI API key for AI brain (required for natural language commands)
OPENAI_API_KEY=sk-your-openai-key-here

# Agent server URL (used by agent.py CLI to connect to agent_server.py)
AGENT_SERVER_URL=http://localhost:8080
